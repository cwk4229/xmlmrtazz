<?xml version="1.0"?>
<map proto="1.3.0">
<name>Spectrumdo</name>
<version>1.0</version>
<objective>Capture all 3 of enemy team's woll and return them to your base.</objective>
<authors>
    <author contribution="Map Design">MrTazz4077</contributor>
	<author contribution="Map Design">dashingdan97</contributor>
</authors>
<contributors>
    <contributor contribution="Feedback and XML Coding">supremegamer</contributor>
</contributors>
<rules>
   <rule>No Team Grefing</rule>
   <rule>No Team Spleefing</rule>
   <rule>No Block Glitching</rule>
</rules>
<teams>
    <team color="blue" max="50">Blue Team</team>
    <team color="dark_red" max="50">Red Team</team>
</teams>
<spawns>
    <spawns kit="spawn">
        <spawn team="blue" yaw="90"<cylinder base="2,3.5,-23" radius="5" height="0"/></spawn>
		<spawn team="red yaw="90"<cylinder base="x(when done),y,x" radius="5" height="0"/></spawn>
        </spawn>
		<default yaw="270"><cylinder base="141,42,150" radius="3" height="0"/></default>
    </spawns>
<kits>
    <kit name="spawn">
        <item slot="0">iron sword</item>
        <item slot="1" enchantment="dig speed:7;arrow infinite:1">bow</item>
        <item slot="28">arrow</item>
        <item slot="2" enchantment="durability:3;dig speed:1">iron pickaxe</item>
        <item slot="4" amount="64">glass</item>
        <item slot="3" amount="64" damage="1">log</item>
		<item slot="6">milk bucket</item>
		<item slot="7">golden apple</item>
    </kit>
</kits>
<regions>
    <apply block="deny-all" message="You may not build outside the playable region!">
    <negative name="playable">
        <rectangle min="95,197" max="188,104"/>
    </negative>
    </apply>
</regions>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>cooked chicken</item>
    <item>ladder</item>
</itemremove>
<maxbuildheight>40</maxbuildheight>
<timelock>on</timelock>
<score>
    <time>600</time>
</score>
</map>
